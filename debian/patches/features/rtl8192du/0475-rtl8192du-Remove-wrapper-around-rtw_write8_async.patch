From a5c9e04e687bc361302781ae095a7f0a0bde3071 Mon Sep 17 00:00:00 2001
From: Larry Finger <Larry.Finger@lwfinger.net>
Date: Thu, 12 Jun 2014 23:38:58 -0500
Subject: [PATCH 475/483] rtl8192du: Remove wrapper around rtw_write8_async

Signed-off-by: Larry Finger <Larry.Finger@lwfinger.net>
---
 core/rtw_io.c    | 5 ++++-
 include/rtw_io.h | 4 +---
 2 files changed, 5 insertions(+), 4 deletions(-)

--- a/drivers/staging/rtl8192du/core/rtw_io.c
+++ b/drivers/staging/rtl8192du/core/rtw_io.c
@@ -139,7 +139,8 @@ int _rtw_writeN(struct rtw_adapter *adap
 
 	return RTW_STATUS_CODE(ret);
 }
-int _rtw_write8_async(struct rtw_adapter *adapter, u32 addr, u8 val)
+
+int rtw_write8_async(struct rtw_adapter *adapter, u32 addr, u8 val)
 {
 	struct io_priv *pio_priv = &adapter->iopriv;
 	struct	intf_hdl		*pintfhdl = &(pio_priv->intf);
@@ -152,6 +153,7 @@ int _rtw_write8_async(struct rtw_adapter
 
 	return RTW_STATUS_CODE(ret);
 }
+
 int rtw_write16_async(struct rtw_adapter *adapter, u32 addr, u16 val)
 {
 	struct io_priv *pio_priv = &adapter->iopriv;
@@ -165,6 +167,7 @@ int rtw_write16_async(struct rtw_adapter
 
 	return RTW_STATUS_CODE(ret);
 }
+
 int rtw_write32_async(struct rtw_adapter *adapter, u32 addr, u32 val)
 {
 	struct io_priv *pio_priv = &adapter->iopriv;
--- a/drivers/staging/rtl8192du/include/rtw_io.h
+++ b/drivers/staging/rtl8192du/include/rtw_io.h
@@ -314,7 +314,7 @@ int _rtw_write16(struct rtw_adapter *ada
 int _rtw_write32(struct rtw_adapter *adapter, u32 addr, u32 val);
 int _rtw_writeN(struct rtw_adapter *adapter, u32 addr, u32 length, u8 *pdata);
 
-int _rtw_write8_async(struct rtw_adapter *adapter, u32 addr, u8 val);
+int rtw_write8_async(struct rtw_adapter *adapter, u32 addr, u8 val);
 int rtw_write16_async(struct rtw_adapter *adapter, u32 addr, u16 val);
 int rtw_write32_async(struct rtw_adapter *adapter, u32 addr, u32 val);
 
@@ -335,8 +335,6 @@ void _rtw_write_port_cancel(struct rtw_a
 #define  rtw_write32(adapter, addr, val) _rtw_write32((adapter), (addr), (val))
 #define  rtw_writeN(adapter, addr, length, data) _rtw_writeN((adapter), (addr), (length), (data))
 
-#define rtw_write8_async(adapter, addr, val) _rtw_write8_async((adapter), (addr), (val))
-
 #define rtw_write_mem(adapter, addr, cnt, mem) _rtw_write_mem((adapter), (addr), (cnt), (mem))
 #define rtw_write_port(adapter, addr, cnt, mem) _rtw_write_port((adapter), (addr), (cnt), (mem))
 #define rtw_write_port_and_wait(adapter, addr, cnt, mem, timeout_ms) _rtw_write_port_and_wait((adapter), (addr), (cnt), (mem), (timeout_ms))
