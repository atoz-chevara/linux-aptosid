From 5d53582d14eb44cd19c0936bc03c475aa191adea Mon Sep 17 00:00:00 2001
From: Larry Finger <Larry.Finger@lwfinger.net>
Date: Sat, 4 May 2013 14:57:19 -0500
Subject: [PATCH 116/210] Remove CONFIG_USE_VMALLOC - always selected

Signed-off-by: Larry Finger <Larry.Finger@lwfinger.net>
---
 include/osdep_service.h | 9 +--------
 1 file changed, 1 insertion(+), 8 deletions(-)

--- a/drivers/staging/rtl8192du/include/osdep_service.h
+++ b/drivers/staging/rtl8192du/include/osdep_service.h
@@ -278,8 +278,6 @@ static inline void rtw_netif_stop_queue(
 
 extern int RTW_STATUS_CODE(int error_code);
 
-#define CONFIG_USE_VMALLOC
-
 //flags used for rtw_update_mem_stat()
 enum {
 	MEM_STAT_VIR_ALLOC_SUCCESS,
@@ -305,15 +303,10 @@ extern void	_rtw_vmfree(u8 *pbuf, u32 sz
 extern u8*	_rtw_zmalloc(u32 sz);
 extern u8*	_rtw_malloc(u32 sz);
 extern void	_rtw_mfree(u8 *pbuf, u32 sz);
-#ifdef CONFIG_USE_VMALLOC
+
 #define rtw_vmalloc(sz)			_rtw_vmalloc((sz))
 #define rtw_zvmalloc(sz)			_rtw_zvmalloc((sz))
 #define rtw_vmfree(pbuf, sz)		_rtw_vmfree((pbuf), (sz))
-#else //CONFIG_USE_VMALLOC
-#define rtw_vmalloc(sz)			_rtw_malloc((sz))
-#define rtw_zvmalloc(sz)			_rtw_zmalloc((sz))
-#define rtw_vmfree(pbuf, sz)		_rtw_mfree((pbuf), (sz))
-#endif //CONFIG_USE_VMALLOC
 #define rtw_malloc(sz)			_rtw_malloc((sz))
 #define rtw_zmalloc(sz)			_rtw_zmalloc((sz))
 #define rtw_mfree(pbuf, sz)		_rtw_mfree((pbuf), (sz))
