This debugging aid is no longer used.

Signed-off-by: Larry Finger <Larry.Finger@lwfinger.net>
---
 core/rtw_mlme_ext.c | 21 +--------------------
 1 file changed, 1 insertion(+), 20 deletions(-)

--- a/drivers/staging/rtl8192du/core/rtw_mlme_ext.c
+++ b/drivers/staging/rtl8192du/core/rtw_mlme_ext.c
@@ -10644,35 +10644,16 @@ exit:
 
 u8 mlme_evt_hdl(struct rtw_adapter *adapt, unsigned char *pbuf)
 {
+	struct evt_priv *pevt_priv = &(adapt->evtpriv);
 	u8 evt_code;
-#ifdef CHECK_EVENT_SEQ
-	u8 evt_seq;
-#endif
 	u16 evt_sz;
 	uint *peventbuf;
 	void (*event_callback) (struct rtw_adapter *dev, u8 *pbuf);
-	struct evt_priv *pevt_priv = &(adapt->evtpriv);
 
 	peventbuf = (uint *)pbuf;
 	evt_sz = (u16) (*peventbuf & 0xffff);
-#ifdef CHECK_EVENT_SEQ
-	evt_seq = (u8) ((*peventbuf >> 24) & 0x7f);
