 hal/usb_ops_linux.c     |  1 -
 include/circ_buf.h      | 27 ----------------
 include/osdep_service.h |  1 -
 include/wireless.h      | 83 -------------------------------------------------
 os_dep/xmit_linux.c     |  1 -
 10 files changed, 118 deletions(-)
 delete mode 100644 include/circ_buf.h
 delete mode 100644 include/wireless.h

